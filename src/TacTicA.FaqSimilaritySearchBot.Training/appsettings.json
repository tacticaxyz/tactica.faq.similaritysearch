{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "EmbeddingSettings": {
    "ModelPath": "models/model.onnx",
    "MaxSequenceLength": 512,
    "EmbeddingDimension": 384
  },
  "ProcessingSettings": {
    "MaxTokensPerChunk": 500,
    "SimilarityThreshold": 0.90,
    "TopKResults": 5,
    "MaxDocumentChunks": 10000
  },
  "DataPaths": {
    "QuestionsAnswersFile": "data/questions_answers.txt",
    "WikiLinksFile": "data/wiki_links.txt",
    "OutputDirectory": "wwwroot/data",
    "ExportedContentDirectory": "data/exported-content"
  }
}
